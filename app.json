{
  "expo": {
    "name": "billd-live-react-native",
    "slug": "billd-live-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app/billd-live-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/app/billd-live-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tabbar/home.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.billdlivereactnative",
      "permissions": [
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.BLUETOOTH",
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.WAKE_LOCK"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "5d2f3860-421d-4d26-b4ea-5070956665e3"
      }
    },
    "plugins": [
      [
        "@config-plugins/react-native-webrtc",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone"
        }
      ]
    ],
    "ios": {
      "bitcode": false,
      "bundleIdentifier": "y"
    }
  }
}
